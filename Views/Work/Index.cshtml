@model IEnumerable<CV_Chandan_Rajendra.Models.Work>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <tr>
        <th >
            #
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Company)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Experience)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Projects)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Place)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Year)
        </th>
        <th>
            Job position
        </th>
        <th>
            Skills
        </th>
        <th>
            Description
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <img src=@Html.DisplayFor(modelItem => item.logo)  width="60" height="30">
        </td>
        <td height="100">
            <a href=@Html.DisplayFor(modelItem => item.Company_link) target="_blank">@Html.DisplayFor(modelItem => item.Company)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Experience)
        </td>
        <td>
            <a href="/Projects/Index"> @Html.DisplayFor(modelItem => item.Projects)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Place)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Job_pos)
        </td>
        <td>
            <a href="/Skills/Index">@Html.DisplayFor(modelItem => item.list_skills)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
    </tr>
}

</table>

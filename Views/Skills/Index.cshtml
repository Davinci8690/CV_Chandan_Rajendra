@model IEnumerable<CV_Chandan_Rajendra.Models.Skills>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-hover">
    <thead>
        <tr class="table-dark">
            <th scope="col"> *</th>
            <th scope="col">Skill</th>
            <th scope="col">Work Experience (Years)</th>
            <th scope="col">Training Place</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Level)</th>
            <th scope="col"> No of projects</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><img src=@Html.DisplayFor(modelItem => item.Image_loc) alt="Python logo" width="80" height="50"></td>
                <td height="75"><a href=@Html.DisplayFor(modelItem => item.Skill_hyperlink) target="_blank">@Html.DisplayFor(modelItem => item.Name)</a></td>
                <td>@Html.DisplayFor(modelItem => item.Experience)</td>
                <td><a href=@Html.DisplayFor(modelItem => item.TP_hyplnk) target="_blank">@Html.DisplayFor(modelItem => item.Training_place)</a></td>
                <td>@Html.DisplayFor(modelItem => item.Level)</td>
                <td><a href="/Projects/Index">@Html.DisplayFor(modelItem => item.No_of_projects)</a></td>


            </tr>

        }

    </tbody>
</table>
